<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.dao.OrderInfoMapper">
    <insert id="add" parameterType="com.demo.entity.OrderInfo">
        insert into order_info
        values (NULL, #{account}, #{message}, #{money}, #{createTime})
    </insert>
</mapper>
